{
  "env": {
      "browser": true,
      "es2021": true,
      "jest": true
  },
  "extends": "standard-with-typescript",
  "overrides": [
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "project": "tsconfig.json"
  },
  "rules": {
      "semi": "off",
      "@typescript-eslint/semi": ["error", "always"],
      "@typescript-eslint/restrict-template-expressions": "off"
  }
}
